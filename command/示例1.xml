<根>
	<命令>
		<名称>示例1</名称>
		<模板>python {脚本文件} {表格文件}</模板>
		<用法>python 程序.py xlsx文件路径</用法>
	</命令>
	<参数组>
		<参数 名称="脚本文件" 类型="路径">
			<提示>程序.py 的路径</提示>
			<必需>true</必需>
			<绑定设置>true</绑定设置>
			<描述>程序.py 的路径</描述>
			<过滤器>Python文件(*.py);;所有文件(*.*)</过滤器>
		</参数>
		<参数 名称="表格文件" 类型="路径">
			<提示>*.xlsx 的路径</提示>
			<多参数>true</多参数>
			<必需>true</必需>
			<过滤器>Excel表格文件(*.xlsx);;所有文件(*.*)</过滤器>
		</参数>
	</参数组>
</根>