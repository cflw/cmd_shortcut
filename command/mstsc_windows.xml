<根>
	<命令>
		<名称>远程桌面连接</名称>
		<平台>windows</平台>
		<模板>mstsc {/admin} {/v}{端口号}</模板>
	</命令>
	<参数组>
		<参数 名称="/admin" 类型="开关">
			<描述>使用管理员登录</描述>
			<模板>/admin</模板>
		</参数>
		<参数 名称="/v">
			<描述>主机名</描述>
			<提示>x.x.x.x</提示>
			<模板>/v:{}</模板>
			<必需>true</必需>
		</参数>
		<参数 名称="端口号" 类型="整数">
			<前置参数>/v</前置参数>
			<提示>3389</提示>
			<模板>:{}</模板>
		</参数>
	</参数组>
</根>